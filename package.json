{
  "name": "next-ts-prisma-graphql-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:server dev:db gen-gql:watch",
    "dev:server": "next dev",
    "setup:db": "prisma migrate dev && prisma db seed",
    "dev:db": "docker-compose up -d",
    "dev:db:stop": "docker-compose down",
    "gen-gql": "graphql-codegen",
    "gen-gql:watch": "graphql-codegen --watch",
    "gen-gql:check": "graphql-codegen --check",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "format:check": "prettier --check src"
  },
  "dependencies": {
    "@apollo/client": "^3.7.5",
    "@apollo/server": "^4.3.2",
    "@as-integrations/next": "^1.2.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@next/font": "13.1.6",
    "@prisma/client": "4.9.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "graphql": "^16.6.0",
    "next": "13.1.6",
    "nextjs-cors": "^2.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/client-preset": "^1.2.6",
    "@graphql-codegen/typescript-resolvers": "^2.7.12",
    "autoprefixer": "^10.4.13",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} --transpile-only prisma/seed.ts"
  },
  "packageManager": "pnpm@7.26.2",
  "engines": {
    "node": ">=16.0.0"
  }
}
